{
  "name": "auto-accept-antigravity",
  "displayName": "AutoAccept-Antigravity",
  "description": "Automatically accept AI agent actions (file edits, terminal commands) in Antigravity Google",
  "version": "0.2.1",
  "icon": "icon.png",
  "publisher": "kaushiksaravanan",
  "author": "Kaushik Saravanan",
  "repository": {
    "type": "git",
    "url": "https://github.com/antigravity/auto-accept.git"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "keywords": [
    "auto-accept",
    "antigravity",
    "agent",
    "ai",
    "automation",
    "auto",
    "accept"
  ],
  "categories": [
    "Machine Learning",
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autoAcceptAgent.toggle",
        "title": "Auto Accept: Toggle"
      }
    ],
    "configuration": {
      "title": "Auto Accept Agent",
      "properties": {
        "autoAcceptAgent.blockedCommands": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "rm -rf /",
            "format",
            "mkfs"
          ],
          "description": "A list of strings or command snippets that, if detected near the 'Run' button, will prevent the agent from automatically accepting."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.90.0",
    "@types/ws": "^8.5.10",
    "@vscode/vsce": "^2.31.0",
    "sharp": "^0.34.5",
    "typescript": "^5.4.0"
  }
}